{
	"extends": "eslint:recommended",
	"globals": {
        "MediumEditor": true
    },
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "forOf": true,
        "modules": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "restParams": true,
        "rest": true,
        "superInFunctions": true,
        "templateStrings": true,
        "experimentalObjectRestSpread": true
    },
    "rules": {
        "semi": 2,
        "jsx-quotes": [2, "prefer-double"],
        "eqeqeq": [2, "smart"],
        "guard-for-in": 2,
        "no-case-declarations": 2,
        "no-else-return": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-implicit-coercion": 2,
        "no-implied-eval": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-native-reassign": 2,
        "no-proto": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 2,
        "wrap-iife": 2,
        "yoda": 2,
        "no-shadow": 2,
        "no-use-before-define": 2,
        "no-const-assign": 2,
        "no-class-assign": 2,
        "prefer-template": 2,
        "prefer-const": 2,
        "prefer-arrow-callback": 2,
        "no-var": 2,
        "no-this-before-super": 2,
        "no-dupe-class-members": 2,
        "no-console": 0
    }
}
